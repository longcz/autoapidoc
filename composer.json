{
    "name": "longcz/autoapidoc",
    "description": "Automatic API Document Generation Package",
    "type": "think-extend",
    "authors": [
        {
            "name": "zhengchanglong"
        }
    ],
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Api\\Doc\\": "src"
        },
        "files": [
            "src/helper.php"
        ]
    },
    "require": {
        "topthink/think-helper": ">=1.0.4",
        "topthink/think-installer": ">=1.0.10"
    },
    "extra": {
        "think-config": {
            "apidoc": "src/config.php"
        }
    }
}
